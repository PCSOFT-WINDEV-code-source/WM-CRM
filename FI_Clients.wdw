  procedures :
   -
     name : ActualiseZR
     procedure_id : 1198228981479783724
     type_code : 14
     code : |1+
      // Résumé : Actualise la liste des contacts
      PROCEDURE ActualiseZR()
      
      // Variable
      sCleRecherche     est une chaine
      nSelection        est un entier = -1
      
      // Ré-initialise
      ZoneRépétéeSupprimeTout(ZR_CONTACTS)
      
      // Requête pour récupérer les contacts selon les filtres
      SELON gnOrdreTriContact
      	CAS 1 // Nom
      		REQ_Contacts.pRubriqueTrie = "NomClient"
      		SAI_RECHERCHE_PAR_NOM..Indication = "recherche par nom"
      		
      	CAS 2 // Société
      		REQ_Contacts.pRubriqueTrie = "Société"
      		SAI_RECHERCHE_PAR_NOM..Indication = "recherche par société"
      		
      	CAS 3 // Ville
      		REQ_Contacts.pRubriqueTrie = "Ville"
      		SAI_RECHERCHE_PAR_NOM..Indication = "recherche par ville"
      		
      	CAS 4 // Type
      		REQ_Contacts.pRubriqueTrie = "Pays"
      		SAI_RECHERCHE_PAR_NOM..Indication = "recherche par pays"
      FIN
      
      SAI_RECHERCHE_PAR_NOM = ""
      
      // Sauvegarde la clé de recherche
      sCleRecherche = REQ_Contacts.pRubriqueTrie
      
      // Exécute la requête
      SI HExécuteRequête(REQ_Contacts) ALORS
      	CONTACT_RemplirZR()
      FIN
     type : 458752
   -
     name : CONTACT_RemplirZR
     procedure_id : 1198228981479914936
     type_code : 14
     code : |1-
      // Résumé : Remplit la zone répétée des contacts en fonction du filtre passé en paramètre
      PROCEDURE CONTACT_RemplirZR( sFiltreCleContient est une chaine = "" )
      
      nIndice           est un entier
      sValeurCle        est une chaine
      sValeurAutre      est une chaine
      
      
      // Alimente la ZR
      POUR TOUT REQ_Contacts 
      	
      	// Détermine la clé de recherche
      	SELON gnOrdreTriContact
      		CAS 1 // Nom
      			sValeurCle   = REQ_Contacts.NomClient + " " + REQ_Contacts.PrénomClient
      			sValeurAutre = REQ_Contacts.Société
      			
      		CAS 2 // Société
      			sValeurCle   = REQ_Contacts.Société
      			sValeurAutre = REQ_Contacts.NomClient + " " + REQ_Contacts.PrénomClient
      			
      		CAS 3 // Ville
      			sValeurCle   = REQ_Contacts.Ville
      			sValeurAutre = REQ_Contacts.NomClient + " " + REQ_Contacts.PrénomClient
      			SI REQ_Contacts.Société<>"" ALORS sValeurAutre += " ("+REQ_Contacts.Société+")"
      			
      		CAS 4 // Pays
      			sValeurCle = REQ_Contacts.Pays
      			sValeurAutre = REQ_Contacts.NomClient + " " + REQ_Contacts.PrénomClient
      			SI REQ_Contacts.Société<>"" ALORS sValeurAutre += " ("+REQ_Contacts.Société+")"
      			
      	FIN
      	
      	//Si l’utilisateur a précisé une recherche, il faut vérifier que les résultats respectent le filtre de l'utilisateur,
      	//si oui, il peut être affiché,
      	//sinon, non
      	bAjouteLigne est un booleen = vrai
      	si sFiltreCleContient <> "" ALORS
      		si position(sValeurCle,sFiltreCleContient,1,SansCasse)=0 ALORS
      			bAjouteLigne = faux
      		FIN
      	FIN
      	
      	si bAjouteLigne alors
      		
      		// Insère une ligne dans la ZR
      		nIndice = ZoneRépétéeAjouteLigne(ZR_CONTACTS, REQ_Contacts.NumClient, sValeurCle, sValeurAutre,
      		REQ_Contacts.Ville + " / " + REQ_Contacts.Pays, REQ_Contacts.AdresseEmail, sValeurCle[[1]])
      		
      	fin
      FIN
     type : 458752
