 menu :
  name : _Menu
  identifier : 0x2a2505a900438be8
  internal_properties : 
  options :
   -
     name : OPT_Valider
     identifier : 0x24bfb692130a87c6
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          //Validation des modifications saisies
          
          //Si on est en mode création d'un nouveau contact, on commence par effacer le contenu des rubriques en mémoire 
          SI gIDClient = NIL_UUID ALORS
          	hraz(client)
          FIN
          
          //On transfère dans les rubriques en mémoire le contenu des champs
          //Cela vaut pour les champs qui sont liés depuis leur fenêtre de description à une rubrique du fichier de la base de données
          EcranVersFichier()
          
          
          //Pour les autres champs non liés depuis leur fenêtre de description, parce qu'il nécessite un traitement par exemple
          //on le fait par programmation >>
          
          // Sélecteur de type de client
          // Sélecteur de civilité
          POUR n = 1 _A_ ZM_TypeProspect..Occurrence
          	
          	//Permet de rendre visible la coche sur la ligne que l'on veut sélectionner
          	//Et uniquement sur cette ligne là
          	SI ZM_TypeProspect[n].IMG_Option..Visible = Vrai ALORS
          		CLIENT.Type = n		
          	FIN
          	
          FIN
          
          // Sélecteur de civilité
          POUR n = 1 _A_ ZM_Civilité..Occurrence
          	
          	//Permet de rendre visible la coche sur la ligne que l'on veut sélectionner
          	//Et uniquement sur cette ligne là
          	SI ZM_Civilité[n].IMG_Option..Visible = Vrai alors
          		CLIENT.Civilité = n		
          	FIN
          fin
          
          
          //et enfin : enregistrement dans la base de données
          si henregistre(client) = vrai alors
          	//Et demande de rafraîchissement de l'IHM
          	Notifier(NOTIF_MODIF_CONTACT,CLIENT.NumClient)
          	ferme()
          FIN
          
         type : 32
   -
     name : OPT_Supprimer
     identifier : 0x24bfb692130b8815
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          //Suppression du contact
          SI OuiNon("Êtes-vous sûr de vouloir supprimer ce contact ?")=Oui ALORS
          	//dans la base de données
          	HSupprime()
          	//Et à l'affichage aussi  
          	Notifier(NOTIF_MODIF_CONTACT,gIDClient)
          	Ferme()
          FIN
         type : 32
   -
     name : OPT_Importer
     identifier : 0x24bfdaa723c17d39
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          SI EnModeSimulateur() OU EnModeEmulateuriOS() _ou_ EnModeEmulateurAndroid() ALORS
          	Info("La sélection de contact depuis les contacts de l'appareil ne fonctionne que sur le périphérique.")
          	RETOUR
          FIN
          
          // Sélectionne le contact
          SI ContactSélectionne() = Vrai ALORS
          	
          	SAI_NOM = mContact.Nom
          	SAI_TELEPHONE = mContact.TéléphoneBureau
          	SAI_FAX = mContact.FaxBureau
          	SAI_MOBILE = mContact.TéléphoneMobile
          	SAI_EMAIL = mContact.Email
          	SAI_PAYS = mContact.PaysBureau
          	SAI_VILLE = mContact.VilleBureau
          	SAI_CP = mContact.CodePostalBureau
          	SAI_ADRESSE = mContact.AdresseBureau
          	SAI_PRENOM = mContact.Prénom
          	SAI_SOCIETE = mContact.Société
          	
          FIN
          
         type : 32
  code_elements :
   type_code : 26
   p_codes : []
