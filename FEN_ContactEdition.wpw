  p_codes :
   -
     code : |1+
      PROCEDURE FEN_ContactEdition(gIDClient est un UUID = Nil_UUID)
   -
     code : |1+
      //Si un identifiant de client a été passé en paramètre, on est en mode modification,
      //on affiche donc les informations de ce client
      
      SI gIDClient <> Nil_UUID  ALORS
      	
      	//On rend visible le bouton supprimer
      	OPT_Supprimer..Visible = Vrai
      	
      	//On recherche dans la base de données le client ayant l'identifiant passé en paramètre à la fenêtre
      	HLitRecherchePremier(CLIENT,NumClient,gIDClient)
      	
      	//Si on le trouve effectivement dans la base de données
      	SI HTrouve(CLIENT) ALORS
      		
      		//On affiche ses informations
      		FichierVersEcran()
      		
      		// Sélecteur de type de client
      		nValeurTypeClient est un entier = CLIENT.Type
      		
      		//Conseil d'optimisation : _A_ ZM_Selecteur..Occurrence 
      		// >> "_A_" au lieu de "A" permet de dire que ZM_Selecteur..Occurrence ne sera évalué qu'une fois au début du parcours
      		POUR n = 1 _à_ ZM_TypeProspect..Occurrence
      		
      			//Permet de rendre visible la coche sur la ligne que l'on veut sélectionner
      			//Et uniquement sur cette ligne là
      			SI n = nValeurTypeClient ALORS
      				ZM_TypeProspect[n].IMG_Option..Visible = Vrai 
      			SINON
      				ZM_TypeProspect[n].IMG_Option..Visible = Faux 
      			FIN
      						
      		FIN
      		
      		
      		nValeurCivilité est un entier = CLIENT.Civilité
      		
      		//Conseil d'optimisation : _A_ ZM_Selecteur..Occurrence 
      		// >> "_A_" au lieu de "A" permet de dire que ZM_Selecteur..Occurrence ne sera évalué qu'une fois au début du parcours
      		POUR n = 1 _à_ ZM_Civilité..Occurrence
      			
      			//Permet de rendre visible la coche sur la ligne que l'on veut sélectionner
      			//Et uniquement sur cette ligne là
      			SI n = nValeurCivilité ALORS
      				ZM_Civilité[n].IMG_Option..Visible = Vrai 
      			SINON
      				ZM_Civilité[n].IMG_Option..Visible = Faux 
      			FIN
      			
      		FIN
      				
      	FIN	
      	
      sinon
      	
      	//On rend invisible le bouton supprimer
      	OPT_Supprimer..Visible = faux
      FIN
      
