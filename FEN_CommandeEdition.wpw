 message_bar :
  internal_properties : CAAAAAgAAAD9NyuKCoPB0kUiJp4gRjRey1FrZAPbzZA4ZGr0WMvzvu0+pXStmeFTmpRsjL3+OAXvmi7P51JSgdpPElPPfNvktL49PpbTQGZ86xKUsNTvFNUCbHClHUNWbRChOg==
 code_elements :
  internal_properties : CAAAAAgAAABfrIi9Psl01WAC9izQtIjq3n9uzhFqBRzAN2LISe2cb53TAj7FUGn/JlmPG4/XWV1iLCNvtl3LEq+N2PQdno9WjOIuyGw=
  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE FEN_CommandeEdition(gnIDCommande est un entier = -1)
      
      gnNumClientFacturation est un UUID = Nil_UUID
      gnNumClientLivraison est un UUID = Nil_UUID
      gbSelContactFacturation est un booleen
      
      STLigneCommande est une structure
      	sReference est une chaine
      	sLibProd est une chaine
      	nQuantite est un entier
      FIN
      
      gtabLigneCommande est un tableau de STLigneCommande
   -
     code : |1-
      
      si gnIDCommande = -1 ALORS
      	
      	// Création
      	
      	nLigne est entier 
      	nLigne = zmlAjouteLigne(ZM_TypeCommande)
      	ZM_TypeCommande[nLigne].LIB_QuoiCréer = "Devis"
      	
      	
      	nLigne = zmlAjouteLigne(ZM_TypeCommande)
      	ZM_TypeCommande[nLigne].LIB_QuoiCréer = "Bon de commande"
      	
      	nValeur est un entier = 1
      	
      	//Conseil d'optimisation : _A_ ZM_Selecteur..Occurrence 
      	// >> "_A_" au lieu de "A" permet de dire que ZM_Selecteur..Occurrence ne sera évalué qu'une fois au début du parcours
      	POUR n = 1 _A_ ZM_TypeCommande..Occurrence
      		
      		//Permet de rendre visible la coche sur la ligne que l'on veut sélectionner
      		//Et uniquement sur cette ligne là
      		SI n = nValeur ALORS
      			ZM_TypeCommande[n].IMG_Option..Visible = Vrai 
      		SINON
      			ZM_TypeCommande[n].IMG_Option..Visible = Faux 
      		FIN
      		
      	FIN
      	
      	OPT_Supprimer..Visible = Faux
      	
      sinon
      	
      	// Edition
      	
      	OPT_Supprimer..Visible = vrai
      	
      	HLitRecherchePremier(COMMANDE,NumCommande,gnIDCommande)
      	si htrouve(COMMANDE) = faux ALORS
      		ferme()
      	FIN
      	
      	LIB_QUOICREER = "Type d'action commerciale" 
      			
      	nLigneTypeActionCom est entier 
      	nLigneTypeActionCom = zmlAjouteLigne(ZM_TypeCommande)
      	
      	
      	SELON COMMANDE.EtatCommande
      		CAS STATUT_DEVIS
      			ZM_TypeCommande[nLigneTypeActionCom].LIB_QuoiCréer =  "Devis"
      			
      		CAS STATUT_BON_DE_COMMANDE
      			ZM_TypeCommande[nLigneTypeActionCom].LIB_QuoiCréer =  "Bon de commande"
      			
      		CAS STATUT_FACTURE
      			ZM_TypeCommande[nLigneTypeActionCom].LIB_QuoiCréer =  "Facture"
      			
      		CAS STATUT_COMMANDE_ANNULEE
      			ZM_TypeCommande[nLigneTypeActionCom].LIB_QuoiCréer = "Commande annulée"
      			
      	FIN
      	
      	
      	
      
      	//Mode de règlement
      	nValeurRèglement est un entier  = cOMMANDE.ModeRèglement
      	
      	//Conseil d'optimisation : _A_ ZM_Selecteur..Occurrence 
      	// >> "_A_" au lieu de "A" permet de dire que ZM_Selecteur..Occurrence ne sera évalué qu'une fois au début du parcours
      	POUR n = 1 _à_ ZM_ModeRèglement..Occurrence
      		
      		//Permet de rendre visible la coche sur la ligne que l'on veut sélectionner
      		//Et uniquement sur cette ligne là
      		SI n = nValeurRèglement ALORS
      			ZM_ModeRèglement[n].IMG_Option2..Visible = Vrai 
      		SINON
      			ZM_ModeRèglement[n].IMG_Option2..Visible = Faux 
      		FIN
      		
      	FIN
      	
      	
      	// Client, facturation et livraison
      	gnNumClientFacturation = commande.NumClient
      	gnNumClientLivraison = commande.NumClientLivraison
      	ActualiseInfoClient()
      		
      	// Initialisation du tableau des lignes de commande
      	pour TOUT LIGNECDE AVEC NumCommande = gnIDCommande
      		stLigne est un STLigneCommande
      		stLigne.sReference = LIGNECDE.Référence
      		stLigne.sLibProd = LIGNECDE.LibProd
      		stLigne.nQuantite = LIGNECDE.Quantité
      		TableauAjoute(gtabLigneCommande,stLigne)
      	FIN
      	ActualiseZRContenu()
      	
      	SAI_ObLivraison = commande.ObservationsLivraison
      FIN
