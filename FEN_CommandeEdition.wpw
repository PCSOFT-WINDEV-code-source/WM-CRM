 menu :
  name : _Menu
  identifier : 0x2a2505a900438be8
  internal_properties : 
  options :
   -
     name : OPT_Valider
     identifier : 0x24bfb92c15f7b28f
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          SI gnIDCommande = -1 ALORS
          	
          	HRAZ(COMMANDE)
          	
          	
          	//Définit le type d'actions commerciales : devis, bon de commande, facture..
          	POUR n = 1 _A_ ZM_TypeCommande..Occurrence
          		//Permet de rendre visible la coche sur la ligne que l'on veut sélectionner
          		//Et uniquement sur cette ligne là
          		SI ZM_TypeCommande[n].IMG_Option..Visible = Vrai ALORS
          			COMMANDE.EtatCommande = n		
          		FIN
          	FIN
          	
          	COMMANDE.DateCommande = DateSys()
          	
          FIN
          
          //Définit le mode de règlement : virement, chèque, carte bancaire
          POUR n = 1 _A_ ZM_ModeRèglement..Occurrence
          	//Permet de rendre visible la coche sur la ligne que l'on veut sélectionner
          	//Et uniquement sur cette ligne là
          	SI ZM_ModeRèglement[n].IMG_Option2..Visible = Vrai ALORS
          		COMMANDE.ModeRèglement = n		
          	FIN
          FIN
          
          COMMANDE.NumClient = gnNumClientFacturation
          COMMANDE.NumClientLivraison = gnNumClientLivraison
          commande.ObservationsLivraison = SAI_ObLivraison
          
          moTotalHT, moTotalTVA sont des monétaires
          
          //Modification ou ajout de la commande dans la base de données
          si henregistre(commande) = vrai ALORS 
          	
          	//Si l'enregistrement dans la base de données est réussi .. 
          	
          	// Mise à jour des lignes de commande
          	HExécuteRequête(REQ_SupprimeLigneCommande,hRequêteDéfaut,COMMANDE.NumCommande)
          	
          	LIGNECDE.NumCommande = COMMANDE.NumCommande
          	LIGNECDE.Livré = Faux
          	
          	POUR TOUT stLigne DE gtabLigneCommande
          		
          		HLitRecherchePremier(PRODUIT,Reference,stLigne.sReference)
          		SI HTrouve() ALORS
          			LIGNECDE.LibProd = PRODUIT.LibProd
          			LIGNECDE.Quantité = stLigne.nQuantite
          			LIGNECDE.PrixVente = PRODUIT.PrixHT * LIGNECDE.Quantité
          			LIGNECDE.Référence = PRODUIT.Reference
          			LIGNECDE.Remise = 0
          			LIGNECDE.TauxTVA = PRODUIT.TauxTVA
          			HAjoute(LIGNECDE)
          			
          			moTotalHT  += LIGNECDE.PrixVente
          			moTotalTVA += (LIGNECDE.PrixVente*PRODUIT.TauxTVA)/100
          			
          		FIN
          	FIN
          	
          	// Mise à jour des totaux dans le fichier COMMANDE
          	COMMANDE.TotalHT = moTotalHT
          	COMMANDE.TotalTVA = moTotalTVA
          	COMMANDE.TotalTTC = moTotalHT + moTotalTVA
          	
          	HModifie(COMMANDE)
          	
          	// Notifie les abonnés de la modification de la	 commande
          	Notifier(NOTIF_MODIF_COMMANDE,COMMANDE.NumCommande)
          FIN
          
          Ferme()
          
         type : 32
   -
     name : OPT_Supprimer
     identifier : 0x24bfb92c15f8b2ed
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          SI OuiNon(ChaîneConstruit("Êtes-vous sûr de vouloir supprimer ce %1",COMMANDE.TypeCommande=1 ? "Devis" SINON "Bon de Commande"))=Oui ALORS
          	HLitRecherchePremier(COMMANDE,NumCommande,gnIDCommande)
          	SI HTrouve() ALORS
          		HSupprime(COMMANDE)
          		Ferme()
          	FIN
          FIN
         type : 32
