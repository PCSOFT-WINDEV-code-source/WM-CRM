  procedures :
   -
     name : Notification
     procedure_id : 2257713644358632705
     type_code : 14
     code : |1-
      // Résumé : Appeler lorsqu'une mise à jour dans l'IHM doit avoir lieu suite à la sélection d'un client ou d'un produit, ici 
      
      PROCEDURE Notification(nNotif, nParamNotif)
      
      selon nNotif
      	cas NOTIF_SELECTION_CLIENT
      		si gbSelContactFacturation ALORS
      			gnNumClientFacturation = nParamNotif
      		sinon
      			gnNumClientLivraison = nParamNotif
      		FIN
      		ActualiseInfoClient()
      		Desabonnement(mafenetre..Nom,NOTIF_SELECTION_CLIENT,"*")
      		
      	cas NOTIF_SELECTION_PRODUIT
      		HLitRecherchePremier(PRODUIT,Reference,nParamNotif)
      		si htrouve() alors
      			nRangProd est un entier = tableaucherche(gtabLigneCommande,tcLinéaire,"sReference",PRODUIT.Reference)
      			si nRangProd>0 ALORS
      				gtabLigneCommande[nRangProd].nQuantite++
      			sinon
      				stLigne est un STLigneCommande
      				stLigne.nQuantite = 1
      				stLigne.sLibProd = PRODUIT.LibProd
      				stLigne.sReference = PRODUIT.Reference
      				tableauajoute(gtabLigneCommande,stLigne)
      			FIN
      			ActualiseZRContenu()
      		fin
      		Desabonnement(MaFenêtre..Nom,NOTIF_SELECTION_PRODUIT,"*")
      FIN
     type : 458752
   -
     name : ActualiseInfoClient
     procedure_id : 2257714438927636080
     type_code : 14
     code : |1-
      // Résumé : Actualise les informations du client dans l'affichage de la fenêtre
      PROCEDURE ActualiseInfoClient()
      
      ZM_Facturation.LIB_Text = infosclientFormate(gnNumClientFacturation)
      ZM_Livraison.LIB_TextLivraison = InfosClientFormate(gnNumClientLivraison = Nil_UUID ? gnNumClientFacturation SINON gnNumClientLivraison)
     type : 458752
   -
     name : ActualiseZRContenu
     procedure_id : 2257983020406028554
     type_code : 14
     code : |1+
      // Résumé : Actualise la liste des produits de la commande dans l'affichage de la fenêtre
      PROCEDURE ActualiseZRContenu()
      
      //On commence par tout effacer
      zmlSupprimeTout(ZM_Produits)
      
      //pour ensuite tout ajouter à nouveau 
      stLigne est un STLigneCommande
      POUR TOUT stLigne DE gtabLigneCommande
      	nLigne est un entier = zmlAjouteLigne(ZM_Produits)
      	ZM_Produits[nLigne].LIB_NomProdLigne = stLigne.sLibProd
      	ZM_Produits[nLigne].SAI_QteLigne = stLigne.nQuantite
      FIN
     type : 458752
