   -
     code : |1+
      procedure FEN_CommandeVisu( gnIDCommande est un entier )
      
      gnEtatAction1 est un entier
      gnEtatAction2 est un entier
      
      //Recherche dans la base de données la commande que l'on souhaite afficher
      HLitRecherchePremier(COMMANDE,NumCommande,gnIDCommande)
      SI HTrouve(COMMANDE) = Faux ALORS
      	Ferme()
      FIN
      
      //Et affiche cette commande
      AfficheCommande()
