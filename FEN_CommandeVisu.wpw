  procedures :
   -
     name : Notification
     procedure_id : 2257699088723857346
     type_code : 14
     code : |1+
      // Résumé : Appeler lorsqu'une mise à jour dans l'IHM doit avoir lieu
      
      PROCEDURE Notification(nNotif, nParamNotif)
      
      si nNotif = NOTIF_MODIF_COMMANDE _et_ nParamNotif = gnIDCommande alors
      	Desabonnement(mafenetre..nom,NOTIF_MODIF_COMMANDE,gnIDCommande)
      	AfficheCommande()
      fin
     type : 458752
   -
     name : AfficheCommande
     procedure_id : 2257699191803096591
     type_code : 14
     code : |1-
      // Résumé : Affichage des informations de la commande
      PROCEDURE AfficheCommande()
      
      SELON COMMANDE.EtatCommande
      	CAS STATUT_DEVIS
      		MaFenêtre..titre = "Devis"
      		LIB_N_de_xxx = ChaîneConstruit("N° de Devis : %1", COMMANDE.NumCommande)
      		OPT_Modifier..Visible = Vrai
      		
      	CAS STATUT_BON_DE_COMMANDE
      		MaFenêtre..Titre = "Bon de commande"
      		LIB_N_de_xxx = ChaîneConstruit("N° de Bon de Commande : %1", COMMANDE.NumCommande)
      		OPT_Modifier..Visible = Vrai
      		
      	CAS STATUT_FACTURE
      		MaFenêtre..Titre = "Facture"
      		LIB_N_de_xxx = ChaîneConstruit("N° de Facture : %1", COMMANDE.NumCommande)
      		OPT_Modifier..Visible = Faux
      		
      	CAS STATUT_COMMANDE_ANNULEE
      		MaFenêtre..Titre = "Commande annulée"
      		LIB_N_de_xxx = ChaîneConstruit("N° de Bon de Commande : %1", COMMANDE.NumCommande)
      		OPT_Modifier..Visible = Faux
      
      FIN
      
      //Informations concernant le client : informations de facturation, de livraison
      LIB_Info_facturation = InfosClientFormate(COMMANDE.NumClient)
      SI COMMANDE.NumClientLivraison=Nil_UUID ALORS
      	LIB_Info_livraison = LIB_Info_facturation
      SINON
      	LIB_Info_livraison = InfosClientFormate(COMMANDE.NumClientLivraison)
      FIN
      LIB_InfosLivraison = COMMANDE.ObservationsLivraison
      
      
      //Montant de la commande
      LIB_Montant = ChaîneConstruit("Montant : %1 TTC (soit %2 HT)", MonetaireFormate(COMMANDE.TotalTTC), MonetaireFormate(COMMANDE.TotalHT))
      
      
      //Lignes de la commande : les produits avec leur quantité, leur prix
      zmlSupprimeTout(ZM_Detail)
      POUR TOUT LIGNECDE AVEC NumCommande = gnIDCommande
      	nNumLigne est un entier = zmlAjouteLigne(ZM_Detail)
      	ZM_Detail[nNumLigne].LIB_LigneProduit = LIGNECDE.LibProd
      	ZM_Detail[nNumLigne].LIB_LigneQuantité = LIGNECDE.Quantité
      	HLitRecherchePremier(PRODUIT,Reference,LIGNECDE.Référence)
      	si htrouve() alors
      		ZM_Detail[nNumLigne].LIB_LignePU = MonetaireFormate(produit.PrixHT)
      	fin
      	ZM_Detail[nNumLigne].LIB_LignePT = MonetaireFormate(LIGNECDE.PrixVente)
      FIN
      
      si enmodeandroid() ALORS
      	OPT_Imprimer..visible = faux
      FIN
      
      //Selon l'état de la commande, on active des options précises
      SELON commande.EtatCommande
      	CAS STATUT_DEVIS
      		OPT_Action1..Visible = Vrai
      		OPT_Action1..Libellé = "Transformer en Bon de Commande"
      		gnEtatAction1 = STATUT_BON_DE_COMMANDE
      		OPT_Action2..Visible = Vrai
      		OPT_Action2..Libellé = "Transformer en Facture"
      		gnEtatAction2 = STATUT_FACTURE
      		
      	CAS STATUT_BON_DE_COMMANDE
      		OPT_Action1..Visible = Vrai
      		OPT_Action1..Libellé = "Transformer en Facture"
      		gnEtatAction1 = STATUT_FACTURE
      		OPT_Action2..Visible = Vrai
      		OPT_Action2..Libellé = "Transformer en Commande Annulée"
      		gnEtatAction2 = STATUT_COMMANDE_ANNULEE
      		
      	CAS STATUT_FACTURE,STATUT_COMMANDE_ANNULEE
      		OPT_Action1..Visible = Faux
      		OPT_Action2..Visible = Faux
      FIN
     type : 458752
