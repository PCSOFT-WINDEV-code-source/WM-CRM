  procedures :
   -
     name : Notification
     procedure_id : 2257612338978133519
     type_code : 14
     code : |1-
      // Résumé : Indique à la fenêtre que le client relié à ce rappel vient d'être modifié, la fenêtre doit donc se mettre à jour
      PROCEDURE Notification( nNotif , nParamNotif )
      
      SELON nNotif
      	CAS NOTIF_SELECTION_CLIENT
      		
      		//On récupère le nouveau client relié, et on affiche ses informations 
      		gIdClient = nParamNotif
      		HLitRecherchePremier(CLIENT,NumClient,gIdClient)
      		si htrouve() ALORS
      			
      			ZM_InfoRappel.LIB_NomClient = ChaîneConstruit("%1 %2 %3",formateCivilité(client.civilité),CLIENT.NomClient,CLIENT.PrénomClient)
      			desAbonnement(MaFenêtre..Nom,NOTIF_SELECTION_CLIENT,"*")
      		FIN
      		
      	AUTRE CAS		
      FIN
