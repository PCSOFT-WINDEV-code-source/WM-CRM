      	//Nombre de rappels du jour
      	REQ_NombreRappelsEnAttente.pIDCollaborateur = gnIDCollaborateur
      	SI HExécuteRequête(REQ_NombreRappelsEnAttente) ALORS
      		gnNbreRappels = REQ_NombreRappelsEnAttente.Comptage
      	FIN
      FIN
      
