  options :
   -
     name : OPT_Valider
     identifier : 0x24bf87b30d796fb4
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          //Un client doit nécessairement être associé au rappel
          SI gIdClient = NIL_UUID ALORS
          	Erreur("Vous devez sélectionner un client à associer au rappel")
          	RETOUR
          FIN
          
          
          //Si on est en train de créer un nouveau rappel, on vide préalablement les rubriques mémoires du fichier de données de rappel 
          SI gnIdRappel = -1 ALORS
          	HRAZ(RAPPEL)
          FIN
          
          //On transfère ensuite les données de l'écran vers les rubriques mémoire
          //Cela concerne les champs qui sont liés à une rubrique du fichier depuis la fenêtre de description du champ
          EcranVersFichier()
          
          //Et pour les autres, on 
          RAPPEL.NumClient = gIdClient
          RAPPEL.IDCollaborateur = COLLABORATEUR.IDCollaborateur
          RAPPEL.bOuvert = Vrai
          
          //On enregistre dans la base de données
          henregistre(rappel)
          
          //On notifie les fenêtres de la modification du rappel
          Notifier(NOTIF_MODIF_RAPPEL,RAPPEL.IDRAPPEL)
          
          Ferme()
         type : 32
   -
     name : OPT_Clôturer
     identifier : 0x24bf87fa0da58664
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          //Demande de clôture du rappel
          SI OuiNon(<§$0000§> ) = Oui ALORS
          	
          	//ON le cherche en base
          	HLitRecherchePremier(RAPPEL,IDRAPPEL,gnIdRappel)
          	
          	SI HTrouve() ALORS
          		//Si on le trouve, on le modifie en base
          		RAPPEL.bOuvert = Faux
          		HModifie(RAPPEL)
          		//Et on notifie de la modification
          		Notifier(NOTIF_MODIF_RAPPEL,gnIdRappel)
          		Ferme()
          	FIN
          	
          FIN
         type : 32
   -
     name : OPT_Supprimer
     identifier : 0x24bf87fa0da686b2
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          //Demande de suppression du rappel
          SI OuiNon("Êtes-vous sûr de vouloir supprimer ce rappel ?") = Oui ALORS
          	
          	//On le cherche en base 
          	HLitRecherchePremier(RAPPEL,IDRAPPEL,gnIdRappel)
          	
          	SI HTrouve() ALORS
          		//Si on le trouve, on le supprime dans la base de données
          		HSupprime(RAPPEL)
          		//Et on notifie de cette suppression
          		Notifier(NOTIF_MODIF_RAPPEL,gnIdRappel)
          		Ferme()
          	FIN
          	
          FIN
         type : 32
