  internal_properties : CAAAAAgAAACXiHbz5sxAgg5oBw263mrRnl5QmeWZ2tcO8dL8nvaYYNJAxuxYRbm37sT8zEv+32HGTo1rg0gLMk+wPSg2esA=
  type_code : 1
  p_codes :
   -
     code : |1+
      procedure FEN_RappelEdition(gnIdRappel est un entier = -1)
      
      gIdClient est un UUID = NIL_UUID
   -
     code : |1+
      //Soit on veut créer un nouveau rappel, soit on veut en afficher un existant
      
      //Si la fenêtre a reçu un identifiant de rappel, c'est que l'on veut l'afficher
      si gnIdRappel <> -1 ALORS
      	
      	//On le recherche dans la base de données
      	HLitRecherchePremier(RAPPEL,IDRAPPEL,gnIdRappel)
      	//Si on le trouve effectivement bien, on l'affiche dans la fenêtre
      	si htrouve() ALORS
      		
      		FichierVersEcran()
      		
      		//Et on affiche les informations du client concerné par ce rappel
      		gIdClient = RAPPEL.NumClient
      		HLitRecherchePremier(CLIENT,NumClient,rappel.NumClient)
      		si htrouve() ALORS
      			ZM_InfoRappel.LIB_NomClient = ChaîneConstruit("%1 %2 %3", FormateCivilité(CLIENT.Civilité),CLIENT.NomClient,CLIENT.PrénomClient)
      		FIN
      		
      	FIN
      	
      sinon
      	
      	//On veut créer un nouveau rappel, on prédéfinit certaines informations
      	SAI_Date_du_rappel = datedujour()
      	hHeure est une heure
      	hHeure..minute = 0
      	hHeure..Heure += 2
      	SAI_Heure_du_rappel = hHeure
      	
      FIN
