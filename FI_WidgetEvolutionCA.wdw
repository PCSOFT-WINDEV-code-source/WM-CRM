     code : |1-
      // Résumé : Actualise le chiffre d'affaires
      PROCEDURE ActualiseChiffreAffaires()
      
      //On supprime les données précédentes du graphe
      grSupprimeSérie(GRF_CA,1,grDonnées)
      
      grCouleurSérie(GRF_CA,1,RVB(163,0,72))
      
      <COMPILE SI TypeConfiguration=iOS>
      	grBulle(GRF_CA,grFormatBulle, "[%CATEGORIE%]" + RC + "[%VALEUR%]" + " K€" ) 
      <fin>
      
      //Puis parcours de la requête pour ajouter les données dans le graphe
      nIndice est un entier
      pour TOUT REQ_CalculCAParJourSurPériode 
      	
      	nIndice ++
      	
      	grEtiquetteCatégorie(GRF_CA,nIndice,DateVersChaîne(REQ_CalculCAParJourSurPériode.DateCommande, "JJ MMM"))
      	
      	//Divisé par 1000, car CA exprimé en K€
      	grAjouteDonnée(GRF_CA, 1, nIndice , arrondi(REQ_CalculCAParJourSurPériode.la_somme_TotalHT / 1000))
      	
      	
      FIN
      
      grDessine(GRF_CA)
