     code : |1-
      // Résumé : Actualise le top 10 des produits
      PROCEDURE ActualiseTop10Produits()
      
      //On supprime les données précédentes du graphe
      grSupprimeSérie(GRF_MeilleurProduit,1, grDonnées)
      
      <compile SI TypeConfiguration=iOS>
      	grBulle(GRF_MeilleurProduit,grFormatBulle, "Réf : " + "[%CATEGORIE%]" + RC + "Quantité : " + "[%VALEUR%]" ) 
      <FIN>
      
      //Puis parcours de la requête pour ajouter les données dans le graphe
      nIndice est un entier
      pour TOUT REQ_Top10Produit DepuisFin
      	
      	nIndice++
      	grEtiquetteCatégorie(GRF_MeilleurProduit,nIndice,REQ_Top10Produit.reference)
      	grajouteDonnée(GRF_MeilleurProduit,1, nIndice, REQ_Top10Produit.TotalQte)	
      	
      FIN
      
      grDessine(GRF_MeilleurProduit)
